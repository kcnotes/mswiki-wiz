{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "wz-reader:default",
    "wz-reader:allow-execute",
    "wz-reader:allow-get-server-url",
    "dialog:default",
    "dialog:allow-open",
    "fs:default",
    "fs:write-all",
    "fs:read-all",
    {
      "identifier": "http:default",
      "allow": [{ "url": "https://maplestorywiki.net" }]
    }
  ]
}